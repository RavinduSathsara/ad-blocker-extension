{
  "manifest_version": 2,
  "name": "Ad Blocker Extension",
  "version": "1.0",
  "description": "A simple ad blocker Chrome extension built with React JS.",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon.ico",
      "48": "favicon.ico",
      "128": "favicon.ico"
    }
  },
  "permissions": ["activeTab"]
}
